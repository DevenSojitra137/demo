<form method="post">
  <table>
    <tr>
      <td>Name:</td>
      <td><input type="text" name="name" /></td>
    </tr>
    <tr>
      <td>Email:</td>
      <td><input type="email" name="email" /></td>
    </tr>
    <tr>
      <td>Password:</td>
      <td><input type="password" name="password" /></td>
    </tr>
    <tr>
        <td></td>
        <td><input type="submit" value="save" id=""></td>
    </tr>
  </table>

  <table border="1">
    <th>Id</th>
    <th>Name</th>
    <th>Email</th>
    <th>Password</th>

    <% result.forEach(function(tbl1){ %>
    <tr>
      <td><%= tbl1.id%></td>
      <td><%= tbl1.name%></td>
      <td><%= tbl1.email%></td>
      <td><%= tbl1.password%></td>
    </tr>
    <% }); %>
  </table>
</form>
